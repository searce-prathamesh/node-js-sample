pipeline {
  agent any
  tools {nodejs "nodejs"}
  stages {
    stage('Git checkout') {
      steps {
        git 'https://github.com/searce-prathamesh/node-js-sample'
      }
    }
    stage('Build') {
      steps {
        sh 'npm install'
        }
    }
    stage('run') {
      steps {
      sh 'npm run'
      }
    }
  }
}

