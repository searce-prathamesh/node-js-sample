pipeline {
  agent any // Use any available Jenkins agent
  environment {
    NODE_ENV = 'production' // Set the desired environment if needed
  }
  stages {
    stage('git Checkout') {
      steps {
        checkout scm
      }
    }
    stage('Install Dependencies') {
      steps {
        script {
            sh 'pnpm install'
        }
      }
    }
    stage('Test') {
      steps {
        sh 'npm test' // Replace with your test command
      }
    }
  }
}

